{
  "name": "react-navigate",
  "version": "0.5.0",
  "description": "Navigation on web for React.JS",
  "main": "lib/index.js",
  "files": [
    "src",
    "lib"
  ],
  "scripts": {
    "test": "standard && node -c lib/index.js",
    "docs": "BABEL_ENV=docs webpack",
    "docs:watch": "BABEL_ENV=docs webpack-dev-server",
    "prebuild": "mkdir -p lib",
    "build": "babel src/Navigation.js > lib/index.js",
    "prepublish": "npm run build"
  },
  "repository": "xiaody/react-navigate",
  "keywords": [
    "react",
    "navigation",
    "controller",
    "navigator",
    "component"
  ],
  "author": "xiaody",
  "license": "MIT",
  "peerDependencies": {
    "prop-types": "*",
    "react": "*",
    "react-dom": "*",
    "react-addons-css-transition-group": "*"
  },
  "dependencies": {
    "throttleit": "1.0.0"
  },
  "devDependencies": {
    "babel-cli": "6.24.1",
    "babel-eslint": "7.2.3",
    "babel-loader": "7.0.0",
    "babel-plugin-transform-class-properties": "6.24.1",
    "babel-plugin-transform-react-jsx": "6.24.1",
    "babel-preset-es2015": "6.24.1",
    "babel-preset-stage-3": "6.24.1",
    "core-js": "2.4.1",
    "css-loader": "0.28.1",
    "prop-types": "*",
    "raf": "3.3.2",
    "react": "*",
    "react-dom": "*",
    "react-addons-css-transition-group": "*",
    "standard": "10.0.2",
    "style-loader": "0.17.0",
    "webpack": "2.5.1",
    "webpack-dev-server": "2.4.5"
  },
  "standard": {
    "parser": "babel-eslint",
    "ignore": [
      "lib",
      "docs/*.bundle.js"
    ]
  }
}
