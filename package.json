{
  "name": "react-navigate",
  "version": "0.6.0",
  "description": "Navigation on web for React.JS",
  "main": "lib/index.js",
  "files": [
    "src",
    "lib"
  ],
  "scripts": {
    "test": "standard && node -c lib/index.js",
    "docs": "BABEL_ENV=docs webpack -p",
    "docs:watch": "BABEL_ENV=docs webpack-dev-server",
    "prebuild": "mkdir -p lib",
    "build": "babel src/Navigation.js > lib/index.js",
    "prepublish": "npm run build"
  },
  "repository": "xiaody/react-navigate",
  "keywords": [
    "react",
    "navigation",
    "controller",
    "navigator",
    "component"
  ],
  "author": "xiaody",
  "license": "MIT",
  "peerDependencies": {
    "react": "*",
    "react-dom": "*",
    "react-transition-group": "1.x",
    "prop-types": "*"
  },
  "dependencies": {
    "throttleit": "1.0.0"
  },
  "devDependencies": {
    "babel-cli": "6.26.0",
    "babel-eslint": "8.0.1",
    "babel-loader": "7.1.2",
    "babel-plugin-transform-class-properties": "6.24.1",
    "babel-plugin-transform-react-jsx": "6.24.1",
    "babel-preset-es2015": "6.24.1",
    "babel-preset-stage-3": "6.24.1",
    "core-js": "2.5.1",
    "css-loader": "0.28.7",
    "prop-types": "15.6.0",
    "raf": "3.4.0",
    "react": "^16.8",
    "react-dom": "^16.8",
    "react-transition-group": "1.2.1",
    "standard": "10.0.3",
    "style-loader": "0.19.0",
    "webpack": "3.7.1",
    "webpack-dev-server": "4.11.1"
  },
  "standard": {
    "parser": "babel-eslint",
    "ignore": [
      "lib",
      "docs/*.bundle.js"
    ]
  }
}
